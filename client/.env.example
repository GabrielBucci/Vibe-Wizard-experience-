# SpacetimeDB Connection Configuration
# Copy this file to .env.local for local development overrides

# SpacetimeDB host (without protocol)
# Local dev: localhost:3000
# Production: maincloud.spacetimedb.com
VITE_SPACETIME_HOST=localhost:3000

# SpacetimeDB module name
# Must match the name used when publishing: spacetime publish -s maincloud <module-name>
VITE_SPACETIME_MODULE_NAME=c20091db4c3087a67e463b95ddaab3bed8914d4288517925554ee9a820c62f5d
